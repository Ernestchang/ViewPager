<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="false">
        <shape>
            <stroke android:width="0.5dip" android:color="#ff979797" />
            <solid android:color="#ffffffff" />
            <corners android:radius="18.799988dip" />
        </shape>
    </item>
    <item android:state_checked="true">
        <shape>
            <stroke android:width="0.5dip" android:color="#ffff5777" />
            <solid android:color="#ffff5777" />
            <corners android:radius="18.799988dip" />
        </shape>
    </item>
</selector>