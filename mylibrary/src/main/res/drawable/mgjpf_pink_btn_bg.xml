<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape>
            <corners android:radius="2.0dip" />
            <solid android:color="@color/mgjpf_act_btn_color_disabled" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <corners android:radius="2.0dip" />
            <solid android:color="@color/mgjpf_act_btn_color_pressed" />
        </shape>
    </item>
    <item>
        <shape>
            <corners android:radius="2.0dip" />
            <solid android:color="@color/mgjpf_act_btn_color_normal" />
        </shape>
    </item>
</selector>